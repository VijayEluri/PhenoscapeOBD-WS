<?xml version="1.0" encoding="UTF-8"?>

<Context docBase="PhenoscapeWeb" path="/PhenoscapeWeb" reloadable="true"
	source="org.eclipse.jst.j2ee.server:PhenoscapeWeb">
	<Resource name="jdbc/phenoscapeDB" auth="Container"
		type="javax.sql.DataSource" maxActive="20" maxIdle="5" maxWait="10000"
		username="?" password="?"
		driverClassName="com.postgresql.jdbc.Driver"
		url="jdbc:postgresql://darwin.nescent.org/obdphenoscape?autoReconnect=true" 
		removeAbandoned="true" removeAbandonedTimeout="30"
		validationQuery="SELECT * FROM obd_schema_metadata;" testWhileIdle="true">
	</Resource>
</Context>
